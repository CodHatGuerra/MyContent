function fatorial(n)
{
    for(var a=n; a=>1 ; a-- )
    {
        n += n*a
        console.log(`Multiplicando ${n} por ${a}`)
    }
    return n
}

console.log(fatorial(5))

function fatorialr(n)
{
    if(n == 1)
    {
        return 1
    }
    else
    {
        return n * fatorialr(n-1)
        
    }
}

console.log(fatorialr(6))